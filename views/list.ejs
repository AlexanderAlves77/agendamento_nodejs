<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="container">
        <h3>Consultas: </h3>
        <form action="/searchresult" method="get">
            <input type="search" name="search" id="search"
                placeholder="Digite o email ou o cpf do cliente">
            <button>Pesquisar</button>
        </form>
        <hr>
        <% appos.forEach(appo => { %>
            <h2>Consulta: <%= appo.description %></h2>
            <h4>Nome: <%= appo.name %></h4>
            <h4>Email: <%= appo.email %></h4>
            <h4>CPF: <%= appo.cpf %></h4>
            <hr>
        <% }) %>
    </div>
    
</body>
</html>